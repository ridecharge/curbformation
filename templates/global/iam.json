{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "IAM Setup v1.0",
  "Parameters": {
    "Environment": {
      "Description": "Name of the environment this VPC represents",
      "Type": "String",
      "MinLength": "4",
      "MaxLength": "14",
      "AllowedPattern": "[a-z0-9]*",
      "ConstraintDescription": "can contain only lower case alphanumeric characters."
    }
  },
  "Resources": {
    "Users": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "Parameters": {
          "Environment": {"Ref": "Environment"},
          "AdminGroup": {
            "Fn::GetAtt": [
              "Groups",
              "Outputs.AdminGroup"
            ]
          }
        },
        "TemplateURL": "https://s3.amazonaws.com/curbformation-global-templates/iam/users.json",
        "TimeoutInMinutes": "30"
      }
    },
    "Groups": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "Parameters": {
          "Environment": {"Ref": "Environment"}
        },
        "TemplateURL": "https://s3.amazonaws.com/curbformation-global-templates/iam/groups.json",
        "TimeoutInMinutes": "30"
      }
    }
  }
}

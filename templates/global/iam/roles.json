{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "IAM Role Setup v1.0",
  "Parameters": {
    "Environment": {
      "Description": "Name of the environment this VPC represents",
      "Type": "String",
      "MinLength": "4",
      "MaxLength": "14",
      "AllowedPattern": "[a-z0-9]*",
      "ConstraintDescription": "can contain only lower case alphanumeric characters."
    }
  },
  "Outputs": {
    "NATInstanceProfile": {
      "Value": {
        "Fn::GetAtt": [
          "NATInstanceProfile",
          "Outputs.RoleInstanceProfile"
        ]
      }
    },
    "TestAppInstanceProfile": {
      "Value": {
        "Fn::GetAtt": [
          "TestAppInstanceProfile",
          "Outputs.RoleInstanceProfile"
        ]
      }
    }
  },
  "Resources": {
    "NATInstanceProfile": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "Parameters": {
          "Environment": "global"
        },
        "TemplateURL": "https://s3.amazonaws.com/curbformation-global-templates/iam/roles/nat_role.json",
        "TimeoutInMinutes": "30"
      }
    },
    "TestAppInstanceProfile": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "Parameters": {
          "Environment": "global"
        },
        "TemplateURL": "https://s3.amazonaws.com/curbformation-global-templates/iam/roles/test_role.json",
        "TimeoutInMinutes": "30"
      }
    }
  }
}

{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "IAM User Setup v1.0",
  "Parameters": {
    "Environment": {
      "Description": "Name of the environment this VPC represents",
      "Type": "String",
      "MinLength": "4",
      "MaxLength": "14",
      "AllowedPattern": "[a-z0-9]*",
      "ConstraintDescription": "can contain only lower case alphanumeric characters."
    },
    "AdminGroup": {
      "Type": "String"
    }
  },
  "Resources": {
    "AdminUsers": {
      "Type": "AWS::CloudFormation::Stack",
      "Properties": {
        "Parameters": {
          "AdminGroup": {
            "Ref": "AdminGroup"
          }
        },
        "TemplateURL": "https://s3.amazonaws.com/curbformation-global-templates/iam/users/admin.json",
        "TimeoutInMinutes": "30"
      }
    }
  }
}
